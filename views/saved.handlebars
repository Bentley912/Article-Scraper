{{>elements/navbar}}

<ul class="list-group">
    {{#each contents}}
  <li class="list-group-item">
    <h4 class="list-group-item-heading">{{this.title}}</h4>
    <h4 class="list-group-item-text">Job Description: {{this.description}}</h4>
    <p class="list-group-item-text"><a href="/jobs/{{this.id}}">View the full page</p></a>
    <p class="list-group-item-text">Posted by: <a href="/users/{{this.Client.username}}/profile">{{this.Client.username}}</p></a>
  </li>
  {{/each}}
  </ul>